<Window x:Class="xstatic.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:xstatic"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label Margin="20" Content="{x:Static Colors.Azure}" />
        <Label Margin="20" Content="{x:Static sys:Environment.CurrentDirectory}" />
        <Label Margin="20" Content="{x:Static sys:DateTime.Now}" />
        <Label Margin="20" Content="{x:Static local:FileExtensions.xml}" />
    </StackPanel>
</Window>
