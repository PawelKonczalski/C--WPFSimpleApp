<Window x:Class="menusAndBuilt_inCommands.HelpBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:menusAndBuilt_inCommands"
        mc:Ignorable="d"
        Title="HelpBox" Height="450" Width="800">
    <StackPanel>
        <TextBlock>Enter you email: </TextBlock>
        <TextBox x:Name="Email"></TextBox>
        <Button Click="Button_Click">Accept</Button>
    </StackPanel>
</Window>
